<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.jkgames.GameAndEngine"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="4"/>
    <application android:label="@string/app_name">
        <activity android:name="MyActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainMenuActivity" android:label="Main Menu Activity" android:configChanges="keyboard|keyboardHidden|orientation"/>
        <activity android:name=".NewGameActivity" android:label="New Game Activity" android:configChanges="keyboard|keyboardHidden|orientation"/>
        <activity android:name=".WorldActivity" android:label="World Activity" android:configChanges="keyboard|keyboardHidden|orientation"/>
    </application>
</manifest> 
